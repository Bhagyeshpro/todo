{
  "expo": {
    "name": "TodoNow",
    "slug": "todo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./todoAssets/logo png.png",
    "splash": {
      "image": "./todoAssets/todo logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "todonow.todo",
      "adaptiveIcon": {
        "foregroundImage": "./todoAssets/todo logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./todoAssets/logo png.png"
    }
  }
}
